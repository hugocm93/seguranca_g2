package model;

/*
 * Possíveis grupos a qual um usuário pertence
 */
public enum Group
{
	ADMIN, USER;

	private String _name;

	static
	{
		OK._name = "(OK)";
		NOT_OK._name = "(NOT OK)";
		NOT_FOUND._name = "(NOT FOUND)";
		COLLISION._name = "(COLLISION)";
		NOT_DEFINED._name = "(NOT_DEFINED)";
	}

	public String getStatusName()
	{
		return _name;
	}
}